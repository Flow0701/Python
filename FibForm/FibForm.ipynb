    "from __future__ import division\n",
    "import math\n",
    "\n",
    "def fib(n):\n",
    "    SQRT5 = math.sqrt(5)\n",
    "    PHI = (SQRT5 + 1) / 2\n",
    "    return int(PHI ** n / SQRT5 + 0.5)"
   ]
  },
